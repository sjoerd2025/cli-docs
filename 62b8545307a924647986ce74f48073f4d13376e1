rn variants[0], nil\n\t}\n\n\treturn \"\", nil\n}\n\n// SetFlag updates or creates a flag in the cache\nfunc (e *Evaluator) SetFlag(ctx context.Context, flag *Flag) error {\n\te.mu.Lock()\n\tdefer e.mu.Unlock()\n\n\te.cache[flag.Key] = flag\n\treturn nil\n}\n\n// GetFlag retrieves a flag from cache\nfunc (e *Evaluator) GetFlag(ctx context.Context, key string) (*Flag, error) {\n\te.mu.RLock()\n\tdefer e.mu.RUnlock()\n\n\tflag, exists := e.cache[key]\n\tif !exists {\n\t\treturn nil, nil\n\t}\n\n\treturn flag, nil\n}\n\n// ClearCache empties the flag cache\nfunc (e *Evaluator) ClearCache() {\n\te.mu.Lock()\n\tdefer e.mu.Unlock()\n\n\te.cache = make(map[string]*Flag)\n}\n",
