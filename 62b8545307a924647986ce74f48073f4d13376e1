            "content": "#!/usr/bin/env node\n\nconst fs = require('fs');\nconst path = require('path');\n\nconst MANIFESTS_DIR = path.join(__dirname, '../manifests');\nconst DOCS_DIR = path.join(__dirname, '../docs');\n\n// Ensure docs directory exists\nif (!fs.existsSync(DOCS_DIR)) {\n  fs.mkdirSync(DOCS_DIR, { recursive: true });\n}\n\n// Read all JSON manifest files\nconst manifestFiles = fs.readdirSync(MANIFESTS_DIR).filter(f => f.endsWith('.json'));\nconst allTools = {};\nconst allIndex = [];\n\nmanifestFiles.forEach(file => {\n  const content = fs.readFileSync(path.join(MANIFESTS_DIR, file), 'utf8');\n  const data = JSON.parse(content);\n\n  if (Array.isArray(data)) {\n    data.forEach(category => {\n      if (category.tools) {\n        allTools[category.category] = {\n          description: category.description,\n          tools: category.tools\n        };\n\n        category.tools.forEach(tool => {\n          allIndex.push({\n            name: tool.name,\n            title: tool.title,\n            category: category.category,\n            description: tool.description\n          });\n        });\n      }\n    });\n  }\n});\n\n// Generate index page\nlet indexContent = `# CLI Tools & Global Packages Reference\n\nComplete reference for installed CLI tools and global packages across npm, yarn, and Go.\n\n## Categories\n\n`;\n\nObject.keys(allTools).forEach(category => {\n  const categoryData = allTools[category];\n  const slug = category.replace(/[^a-z0-9-]/gi, '-').toLowerCase();\n  indexContent += `- [${category}](./${slug}.md) - ${categoryData.description}\\n`;\n});\n\nfs.writeFileSync(path.join(DOCS_DIR, 'index.md'), indexContent);\nconsole.log('✓ Generated docs/index.md');\n\n// Generate category pages\nObject.keys(allTools).forEach(category => {\n  const categoryData = allTools[category];\n  const slug = category.replace(/[^a-z0-9-]/gi, '-').toLowerCase();\n\n  let content = `# ${category}\\n\\n${categoryData.description}\\n\\n`;\n\n  categoryData.tools.