nable flag in cache\nfunc (wc *WorkflowChain) EnableFlagForUser(ctx context.Context, flagKey string) error {\n\twc.logger.Printf(\"[WORKFLOW] Step 3: Enable flag '%s'\", flagKey)\n\n\tflag := &flags.Flag{\n\t\tKey:       flagKey,\n\t\tEnabled:   true,\n\t\tVariants:  map[string]string{},\n\t\tCreatedAt: \"2024-01-01T00:00:00Z\",\n\t}\n\n\tif err := wc.evaluator.SetFlag(ctx, flag); err != nil {\n\t\treturn fmt.Errorf(\"set flag failed: %w\", err)\n\t}\n\n\twc.logger.Printf(\"[WORKFLOW] → Flag '%s' enabled\", flagKey)\n\treturn nil\n}\n\n// ValidateFlag is an executable workflow step: validate flag state\nfunc (wc *WorkflowChain) ValidateFlag(ctx context.Context, flagKey, userID string) error {\n\twc.logger.Printf(\"[WORKFLOW] Step 4: Validate flag '%s'\", flagKey)\n\n\tflag, err := wc.evaluator.GetFlag(ctx, flagKey)\n\tif err != nil {\n\t\treturn fmt.Errorf(\"get flag failed: %w\", err)\n\t}\n\n\tif flag == nil {\n\t\treturn fmt.Errorf(\"flag not found\")\n\t}\n\n\tenabled, _ := wc.evaluator.IsEnabled(ctx, flagKey, userID)\n\twc.logger.Printf(\"[WORKFLOW] → Flag state: Enabled=%v, Key=%s\", enabled, flag.Key)\n\n\treturn nil\n}\n\n// CompleteWorkflow executes the full workflow chain\nfunc (wc *WorkflowChain) CompleteWorkflow(ctx context.Context) error {\n\twc.logger.Println(\"[WORKFLOW] ════════════════════════════════════════════\")\n\twc.logger.Println(\"[WORKFLOW] Starting Feature Flag Workflow Chain\")\n\twc.logger.Println(\"[WORKFLOW] ════════════════════════════════════════════\")\n\n\t// Step 1: Check initial flag state (should be disabled/missing)\n\tif err := wc.CheckUserFlag(ctx, \"new-feature\", \"user-123\"); err != nil {\n\t\treturn fmt.Errorf(\"step 1 failed: %w\", err)\n\t}\n\n\t// Step 2: AI suggests relevant flags for a new feature context\n\tif wc.aiClient != nil {\n\t\tif err := wc.SuggestFlagsForFeature(ctx, \"e-commerce checkout redesign\"); err != nil {\n\t\t\twc.logger.Printf(\"[WORKFLOW] ⚠ Step 2 skipped: %v (AI not configured)\", err)\n\t\t}\n\t}\n\n\t// Step 3: Enable the flag\n\tif err := wc.EnableFlagForUser(ctx, \"new-feature\"); err != nil {\n\t\treturn fmt.Errorf(\"step 3 failed: %w\", err)\n\t}\n\n\t// Step 4: Validate the flag is now enabled\n\tif err := wc.ValidateFlag(ctx, \"new-feature\", \"user-123\"); err != nil {\n\t\treturn fmt.Errorf(\"step 4 failed: %w\", err)\n\t}\n\n\twc.logger.Println(\"[WORKFLOW] ════════════════════════════════════════════\")\n\twc.logger.Println(\"[WORKFLOW] ✓ Workflow completed successfully\")\n\twc.logger.Println(\"[WORKFLOW] ════════════════════════════════════════════\")\n\n\treturn nil\n}\n\n// ClearAndReset is an executable workflow step: reset state\nfunc (wc *WorkflowChain) ClearAndReset(ctx context.Context) error {\n\twc.logger.Println(\"[WORKFLOW] Clearing cache and resetting state...\")\n\twc.evaluator.ClearCache()\n\treturn nil\n}\n",
