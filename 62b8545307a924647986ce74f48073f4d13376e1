            "content": "#!/usr/bin/env node\n\n/**\n * CLI Documentation Generator\n *\n * Generates CLI reference docs from a JSON manifest using the template pattern.\n * Usage: npx tsx scripts/generate-cli-docs.ts <manifest.json> <output-dir>\n */\n\nimport fs from 'fs';\nimport path from 'path';\n\ninterface CliCommand {\n  cmd: string;\n  description: string;\n}\n\ninterface CliToolConfig {\n  name: string;\n  title: string;\n  description: string;\n  whatItDoes: string;\n  commands: CliCommand[];\n  docsUrl: string;\n}\n\ninterface ManifestEntry {\n  category: string;\n  tools: CliToolConfig[];\n}\n\nconst CLI_TEMPLATE = (config: CliToolConfig): string => {\n  const commandRows = config.commands\n    .map(c => `| \\`${c.cmd}\\` | ${c.description} |`)\n    .join('\\n');\n\n  return `---\ntitle: ${config.title}\ndescription: ${config.description}\n---\n\n## What it does\n\n${config.whatItDoes}\n\n## Key Commands\n\n| Command | Description |\n|---------|-------------|\n${commandRows}\n\n## Docs\n\n[Official documentation](${config.docsUrl})\n`;\n};\n\nasync function generateDocs(manifestPath: string, outputDir: string) {\n  console.log(`📖 Generating CLI docs from: ${manifestPath}`);\n\n  if (!fs.existsSync(manifestPath)) {\n    console.error(`❌ Manifest not found: ${manifestPath}`);\n    process.exit(1);\n  }\n\n  const manifest = JSON.parse(\n    fs.readFileSync(manifestPath, 'utf-8')\n  ) as ManifestEntry[];\n\n  let totalGenerated = 0;\n\n  for (const entry of manifest) {\n    const categoryDir = path.join(outputDir, entry.category);\n\n    if (!fs.existsSync(categoryDir)) {\n      fs.mkdirSync(categoryDir, { recursive: true });\n      console.log(`📁 Created directory: ${categoryDir}`);\n    }\n\n    for (const tool of entry.tools) {\n      const fileName = `${tool.name}.md`;\n      const filePath = path.join(categoryDir, fileName);\n      const content = CLI_TEMPLATE(tool);\n\n      fs.writeFileSync(filePath, content);\n      console.log(`✅ Generated: ${categoryDir}/${fileName}`);\n      totalGenerated++;\n    }\n  }\n\n  console.log(`\\n🎉 Generated ${totalGenerated} CLI reference docs`);\n  console.log(`📍 Output directory: ${outputDir}`);\n}\n\n// Main execution\nconst [manifestPath, outputDir] = process.argv.slice(2);\n\nif (!manifestPath || !outputDir) {\n  console.error('Usage: npx tsx scripts/generate-cli-docs.ts <manifest.json> <output-dir>');\n  process.exit(1);\n}\n\ngenerateDocs(manifestPath, outputDir).catch(err => {\n  console.error('❌ Error:', err.message);\n  process.exit(1);\n});\n",
