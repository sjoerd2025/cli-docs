Encoder(w).Encode(CheckFlagResponse{\n\t\t\tError: \"invalid request: \" + err.Error(),\n\t\t})\n\t\treturn\n\t}\n\n\t// Evaluate flag\n\tenabled, err := s.evaluator.IsEnabled(r.Context(), req.FlagKey, req.UserID)\n\tif err != nil {\n\t\tw.WriteHeader(http.StatusInternalServerError)\n\t\tjson.NewEncoder(w).Encode(CheckFlagResponse{\n\t\t\tError: \"evaluation failed: \" + err.Error(),\n\t\t})\n\t\treturn\n\t}\n\n\t// Get variant if flag is enabled\n\tvariant := \"\"\n\tif enabled {\n\t\tv, _ := s.evaluator.GetVariant(r.Context(), req.FlagKey, req.UserID)\n\t\tvariant = v\n\t}\n\n\tw.WriteHeader(http.StatusOK)\n\tjson.NewEncoder(w).Encode(CheckFlagResponse{\n\t\tFlagKey: req.FlagKey,\n\t\tEnabled: enabled,\n\t\tVariant: variant,\n\t})\n}\n\n// handleGetFlag retrieves a specific flag\nfunc (s *Server) handleGetFlag(w http.ResponseWriter, r *http.Request) {\n\tw.Header().Set(\"Content-Type\", \"application/json\")\n\n\tkey := mux.Vars(r)[\"key\"]\n\tflag, err := s.evaluator.GetFlag(r.Context(), key)\n\tif err != nil {\n\t\tw.WriteHeader(http.StatusInternalServerError)\n\t\tjson.NewEncoder(w).Encode(map[string]string{\"error\": err.Error()})\n\t\treturn\n\t}\n\n\tif flag == nil {\n\t\tw.WriteHeader(http.StatusNotFound)\n\t\tjson.NewEncoder(w).Encode(map[string]string{\"error\": \"flag not found\"})\n\t\treturn\n\t}\n\n\tw.WriteHeader(http.StatusOK)\n\tjson.NewEncoder(w).Encode(flag)\n}\n\n// handleListFlags returns all cached flags\nfunc (s *Server) handleListFlags(w http.ResponseWriter, r *http.Request) {\n\tw.Header().Set(\"Content-Type\", \"application/json\")\n\n\t// TODO: Implement proper flag listing from Flagsmith API\n\tw.WriteHeader(http.StatusOK)\n\tjson.NewEncoder(w).Encode(map[string]interface{}{\n\t\t\"flags\": []interface{}{},\n\t\t\"total\": 0,\n\t})\n}\n\n// Router returns the configured HTTP router\nfunc (s *Server) Router() *mux.Router {\n\treturn s.router\n}\n",
