ypeExtractor) ExtractFromValue(v interface{}, pkg string) *TypeInfo {\n\trv := reflect.ValueOf(v)\n\trt := reflect.TypeOf(v)\n\n\tinfo := &TypeInfo{\n\t\tName:       rt.Name(),\n\t\tPackage:    pkg,\n\t\tKind:       rt.Kind().String(),\n\t\tIsExported: rt.IsExported(),\n\t\tFields:     []FieldInfo{},\n\t\tMethods:    []MethodInfo{},\n\t}\n\n\t// Extract struct fields\n\tif rt.Kind() == reflect.Struct || (rt.Kind() == reflect.Ptr && rt.Elem().Kind() == reflect.Struct) {\n\t\telemType := rt\n\t\tif rt.Kind() == reflect.Ptr {\n\t\t\telemType = rt.Elem()\n\t\t}\n\n\t\tfor i := 0; i < elemType.NumField(); i++ {\n\t\t\tfield := elemType.Field(i)\n\t\t\tfieldType := field.Type\n\t\t\tisPointer := fieldType.Kind() == reflect.Ptr\n\n\t\t\tfieldInfo := FieldInfo{\n\t\t\t\tName:       field.Name,\n\t\t\t\tType:       fieldType.String(),\n\t\t\t\tIsExported: field.IsExported(),\n\t\t\t\tIsPointer:  isPointer,\n\t\t\t\tJSONTag:    field.Tag.Get(\"json\"),\n\t\t\t}\n\t\t\tinfo.Fields = append(info.Fields, fieldInfo)\n\t\t}\n\t}\n\n\t// Extract methods\n\tfor i := 0; i < rt.NumMethod(); i++ {\n\t\tmethod := rt.Method(i)\n\t\tmethodInfo := MethodInfo{\n\t\t\tName:       method.Name,\n\t\t\tReceiver:   rt.Name(),\n\t\t\tParameters: []string{},\n\t\t\tReturns:    []string{},\n\t\t}\n\n\t\t// Extract parameter and return types\n\t\tmethodType := method.Type\n\t\tfor j := 1; j < methodType.NumIn(); j++ {\n\t\t\tmethodInfo.Parameters = append(methodInfo.Parameters, methodType.In(j).String())\n\t\t}\n\t\tfor j := 0; j < methodType.NumOut(); j++ {\n\t\t\tmethodInfo.Returns = append(methodInfo.Returns, methodType.Out(j).String())\n\t\t}\n\n\t\tinfo.Methods = append(info.Methods, methodInfo)\n\t}\n\n\t// Add validations based on type\n\tinfo.Validations = te.deriveValidations(info)\n\n\tte.types[info.Name] = info\n\treturn info\n}\n\n// deriveValidations infers validation rules from type structure\nfunc (te *TypeExtractor) deriveValidations(info *TypeInfo) []ValidationRule {\n\tvar rules []ValidationRule\n\n\tfor _, field := range info.Fields {\n\t\t// Check for required fields (no pointer, non-empty name)\n\t\tif !field.IsPointer && field.Name != \"\" {\n\t\t\trules = append(rules, ValidationRule{\n\t\t\t\tField:       field.Name,\n\t\t\t\tRule:        \"required\",\n\t\t\t\tDescription: fmt.Sprintf(\"Field %s must be provided\", field.Name),\n\t\t\t})\n\t\t}\n\n\t\t// String validations\n\t\tif field.Type == \"string\" {\n\t\t\trules = append(rules, ValidationRule{\n\t\t\t\tField:       field.Name,\n\t\t\t\tRule:        \"string\",\n\t\t\t\tDescription: fmt.Sprintf(\"Field %s must be a valid string\", field.Name),\n\t\t\t})\n\t\t}\n\n\t\t// Numeric validations\n\t\tif isNumericType(field.Type) {\n\t\t\trules = append(rules, ValidationRule{\n\t\t\t\tField:       field.Name,\n\t\t\t\tRule:        \"numeric\",\n\t\t\t\tDescription: fmt.Sprintf(\"Field %s must be a valid number\", field.Name),\n\t\t\t})\n\t\t}\n\n\t\t// Boolean validations\n\t\tif field.Type == \"bool\" {\n\t\t\trules = append(rules, ValidationRule{\n\t\t\t\tField:       field.Name,\n\t\t\t\tRule:        \"boolean\",\n\t\t\t\tDescription: fmt.Sprintf(\"Field %s must be true or false\", field.Name),\n\t\t\t})\n\t\t}\n\t}\n\n\treturn rules\n}\n\n// GetAllTypes returns all extracted types\nfunc (te *TypeExtractor) GetAllTypes() []*TypeInfo {\n\tvar types []*TypeInfo\n\tfor _, t := range te.types {\n\t\ttypes = append(types, t)\n\t}\n\n\t// Sort by name for consistency\n\tsort.Slice(types, func(i, j int) bool {\n\t\treturn types[i].Name < types[j].Name\n\t})\n\n\treturn types\n}\n\n// ExportJSON exports all types as JSON\nfunc (te *TypeExtractor) ExportJSON() (string, error) {\n\ttypes := te.GetAllTypes()\n\tdata := map[string]interface{}{\n\t\t\"types\":  types,\n\t\t\"count\":  len(types),\n\t\t\"export\": map[string]interface{}{\n\t\t\t\"format\":    \"json-schema\",\n\t\t\t\"timestamp\": \"2026-02-25\",\n\t\t},\n\t}\n\n\tjsonData, err := json.MarshalIndent(data, \"\", \"  \")\n\tif err != nil {\n\t\treturn \"\", err\n\t}\n\n\treturn string(jsonData), nil\n}\n\n// GenerateDocumentation creates markdown documentation for all types\nfunc (te *TypeExtractor) GenerateDocumentation() string {\n\tdoc := \"# Type System Reference\\n\\n\"\n\tdoc += fmt.Sprintf(\"**Generated:** 2026-02-25\\n\\n\")\n\tdoc += fmt.Sprintf(\"**Total Types:** %d\\n\\n\", len(te.types))\n\n\ttypes := te.GetAllTypes()\n\n\tfor _, t := range types {\n\t\tdoc += fmt.Sprintf(\"## %s\\n\\n\", t.Name)\n\t\tdoc += fmt.Sprintf(\"**Package:** `%s`\\n\\n\", t.Package)\n\t\tdoc += fmt.Sprintf(\"**Kind:** %s\\n\\n\", t.Kind)\n\n\t\tif len(t.Fields) > 0 {\n\t\t\tdoc += \"### Fields\\n\\n\"\n\t\t\tdoc += \"| Name | Type | Required | JSON Tag |\\n\"\n\t\t\tdoc += \"|------|------|----------|----------|\\n\"\n\t\t\tfor _, f := range t.Fields {\n\t\t\t\trequired := \"No\"\n\t\t\t\tif !f.IsPointer {\n\t\t\t\t\trequired = \"Yes\"\n\t\t\t\t}\n\t\t\t\tdoc += fmt.Sprintf(\"| `%s` | `%s` | %s | `%s` |\\n\",\n\t\t\t\t\tf.Name, f.Type, required, f.JSONTag)\n\t\t\t}\n\t\t\tdoc += \"\\n\"\n\t\t}\n\n\t\tif len(t.Methods) > 0 {\n\t\t\tdoc += \"### Methods\\n\\n\"\n\t\t\tfor _, m := range t.Methods {\n\t\t\t\tparams := \"\"\n\t\t\t\tif len(m.Parameters) > 0 {\n\t\t\t\t\tparams = \"(\" + fmt.Sprintf(\"%v\", m.Parameters)[1:len(fmt.Sprintf(\"%v\", m.Parameters))-1] + \")\"\n\t\t\t\t}\n\t\t\t\treturns := \"\"\n\t\t\t\tif len(m.Returns) > 0 {\n\t\t\t\t\treturns = \" \" + fmt.Sprintf(\"%v\", m.Returns)[1:len(fmt.Sprintf(\"%v\", m.Returns))-1]\n\t\t\t\t}\n\t\t\t\tdoc += fmt.Sprintf(\"- `%s%s%s`\\n\", m.Name, params, returns)\n\t\t\t}\n\t\t\tdoc += \"\\n\"\n\t\t}\n\n\t\tif len(t.Validations) > 0 {\n\t\t\tdoc += \"### Validations\\n\\n\"\n\t\t\tfor _, v := range t.Validations {\n\t\t\t\tdoc += fmt.Sprintf(\"- **%s** (`%s`): %s\\n\", v.Field, v.Rule, v.Description)\n\t\t\t}\n\t\t\tdoc += \"\\n\"\n\t\t}\n\n\t\tdoc += \"---\\n\\n\"\n\t}\n\n\treturn doc\n}\n\n// Helper function to check if a type is numeric\nfunc isNumericType(typeStr string) bool {\n\tnumericTypes := []string{\"int\", \"int32\", \"int64\", \"uint\", \"uint32\", \"uint64\", \"float32\", \"float64\"}\n\tfor _, t := range numericTypes {\n\t\tif typeStr == t {\n\t\t\treturn true\n\t\t}\n\t}\n\treturn false\n}\n",
